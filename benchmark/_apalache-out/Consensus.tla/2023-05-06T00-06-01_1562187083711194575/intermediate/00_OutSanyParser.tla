--------------------------- MODULE 00_OutSanyParser ---------------------------

EXTENDS Integers, Sequences, FiniteSets, TLC, Apalache

CONSTANT
  (*
    @type: Set(VALUE);
  *)
  Value

VARIABLE
  (*
    @type: Set(VALUE);
  *)
  chosen

(*
  ***********************************************************************
 The set of all values that have been chosen.                          
***********************************************************************
*************************************************************************
 The type-correctness invariant.                                         
*************************************************************************
*)
TypeOK == chosen \in SUBSET Value

InitializeVALUE == Value = { "v1_OF_VALUE", "v2_OF_VALUE", "v3_OF_VALUE" }

(*
  *************************************************************************
 The initial predicate and next-state relation.                          
*************************************************************************
*)
Init == chosen = {}

Next == chosen = {} /\ (\E v \in Value: chosen' = {v})

(*
  *************************************************************************
 Liveness: A value is eventually chosen.                                 
*************************************************************************
*)
Success == <>(chosen /= {})

(*
  *************************************************************************
 The complete spec.                                                      
*************************************************************************
*)
Spec == Init /\ []([Next]_chosen)

(*
  *************************************************************************
 Safety: At most one value is chosen.                                    
*************************************************************************
*)
Inv == TypeOK /\ Cardinality(chosen) <= 1

NOTInv == ~TypeOK

LiveSpec == Spec /\ WF_chosen(Next)

================================================================================
